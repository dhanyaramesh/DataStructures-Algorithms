package hackerRank;

public class reverseLL {
	
	public class Node{
		int data;
		Node next;
		Node(int data){
			this.data = data;
			this.next=null;
			
		}
		
	   public Node insert(Node head,int d){
		   if(head==null){
			   head = new Node(d);
			   return head;
		   }
		   while(head.next!=null){
			   head=head.next;
		   }
		   Node newNode = new Node(d);
		   head.next =newNode;
		   return head;
		   
	   }
	}

	public static void main(String[] args) {
      
      Node h = new Node(1);
      

	}

}
